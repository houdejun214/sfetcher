<?xml version="1.0"?>
<configuration>
	<!-- crawl site for dianping -->
	<site name="dianping" hosturl="http://http://www.gothere.sg/"
		fetcher="com.sdata.component.fetcher.GothereFetcher" parser="com.sdata.component.parser.GothereParser"
		storer="com.sdata.component.data.SdataAddressDbStorer">
		<property>
			<name>source</name>
			<value>gothere</value>
		</property>
		<property>
			<name>MainCrawlThreadNum</name>
			<value>2</value>
		</property>
		<property>
			<name>FetchThreadNum</name>
			<value>1</value>
		</property>
		<property>
			<name>mongoHost</name>
			<value>172.18.109.15</value>
		</property>
		<property>
			<name>indexCollection</name>
			<value>address</value>
		</property>
		<property>
			<name>mongoPort</name>
			<value>27017</value>
		</property>
		<property>
			<name>mongoDbName</name>
			<value>gothere</value>
		</property>
		<property>
			<name>TaskInterval</name>
			<value>0</value>
		</property>
		<property>
			<name>datumXml</name>
			<value>conf/column-maps-gothere.xml</value>
		</property>
	</site>
</configuration>