<?xml version="1.0"?>
<configuration>
	
	<site name="weibo" hosturl="www.weibo.com" fetcher="com.sdata.component.fetcher.WeiboFetcher"
		storer="com.sdata.component.data.storer.SdataTweetsStorer">
		<property>
			<name>FetchThreadNum</name>
			<value>1</value>
		</property>
		<property>
			<name>FetchWeiboNum</name>
			<value>200</value>
		</property>
		<property>
			<name>source</name>
			<value>weibo</value>
		</property>
		<property>
			<name>FetchSleepInterval</name>
			<value>25000</value>
		</property>
		<property>
			<name>crawler.index</name>
			<value>true</value>
			<description></description>
		</property>
		<property>
			<name>elastic.cluster.name</name>
			<value>ugcd</value>
			<description></description>
		</property>
		<property>
			<name>datumXml</name>
			<value>template/weibo/tpl-weibo-datum.xml</value>
		</property>
		<property>
			<name>storeXml</name>
			<value>template/weibo/tpl-weibo-store.xml</value>
		</property>
	</site>
	
</configuration>