<?xml version="1.0"?>
<store>
	<!-- 
	如果有多个collection，配置文件中内部field的子集合一定要放到前面，datum的大集合放到配置文件的最后面
	<collection name="posts" primyKey="_id" >
		<update>reviews</update>
		<remove>user</remove>
	</collection> 
	-->
	<collection name="reviews" primaryKey="_id" field="reviews"></collection>
	<collection name="shops" primaryKey="_id" index="true">
		<remove>reviews</remove>
	</collection>
</store>