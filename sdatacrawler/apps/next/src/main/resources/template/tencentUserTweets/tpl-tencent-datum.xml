	<field from="id" name="_id"></field>
	<field from="id" name="id"></field>
	<field from="text" name="text"></field>
	<field from="name" name="sname" desc="发表人帐户名"></field>
	<field from="name" name="uid" handler="com.sdata.core.parser.html.handler.tencent.TencentUidHandler"></field>
	<field from="nick" name="name" desc="：发表人昵称"></field>
	<field from="count" name="retct" desc="微博被转次数"></field>
	<field from="mcount" name="comct" desc="点评次数"></field>
	<field from="from" name="sourc" desc="来源"></field>
	<field from="fromurl" name="fromu" desc="来源"></field>
	<field from="image" name="image" desc="图片url列表"></field>
	<field from="video" name="vd">
		<field from="picurl" name="pu" desc="缩略图"></field>
		<field from="player" name="py" desc="播放器地址"></field>
		<field from="realurl" name="ru" desc="视频原地址"></field>
		<field from="shorturl" name="su" desc="视频的短url"></field>
		<field from="title" name="tt" desc="视频标题"></field>
	</field>
