<?xml version="1.0"?>
<datums>
    <datum match="zappos.com">
        <field name="domain" value="zappos.com" />
        <field name="category" selector="{{category}}" />
        <field name="url" selector="{{url}}" />
        <field name="productId" selector="{{productId}}" />
        <field name="image" selector="#protagonist img,#detailImage img|link"></field>
        <field name="title" selector="#productStage h1.banner|text"></field>
    </datum>

