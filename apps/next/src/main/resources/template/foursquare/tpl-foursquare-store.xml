<?xml version="1.0"?>
<store>
	<!-- 
	如果有多个collection，配置文件中内部field的子集合一定要放到前面，datum的大集合放到配置文件的最后面
	<collection name="posts" primyKey="_id" >
		<update>reviews</update>
		<remove>user</remove>
	</collection> 
	-->
	<collection name="foursquare_venues" primaryKey="id" field="venue">
	</collection>
	<collection name="foursquare_users" primaryKey="id" field="user">
	</collection>
	<collection name="foursquare_checkins" primaryKey="id" index="true">
		<remove>user</remove>
		<remove>venue</remove>
	</collection>
</store>