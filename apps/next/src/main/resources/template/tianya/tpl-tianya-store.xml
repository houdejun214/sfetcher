<?xml version="1.0"?>
<store>
	<!-- 
	如果有多个collection，配置文件中内部field的子集合一定要放到前面，datum的大集合放到配置文件的最后面
	<collection name="posts" primyKey="_id" field="replys" >
		<update>reviews</update>
		<remove>user</remove>
	</collection> 
	-->
	<collection name="replys" primaryKey="rid" field="replys"></collection>
	<collection name="posts" primaryKey="_id" index="true">
		<remove>replys</remove>
	</collection>
</store>