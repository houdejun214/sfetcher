<?xml version="1.0"?>
<configuration>
	<!-- flickr photo's favorites -->
	<site name="flickrFavorites" hosturl="www.flickr.com"
		fetcher="com.sdata.component.fetcher.FlickrFavoritesFetcher" storer="com.sdata.component.data.SdataFlickrFavoritesDbStorer">
		<property>
			<name>QueueThresholdSizeSlow</name>
			<value>400</value>
			<description></description>
		</property>
		<property>
			<name>source</name>
			<value>flickr</value>
		</property>
		<property>
			<name>FetchThreadNum</name>
			<value>15</value>
		</property>
		<property>
			<name>MainCrawlThreadNum</name>
			<value>1</value>
		</property>
		<property>
			<name>maxFavoritesNum</name>
			<value>1000</value>
			<description></description>
		</property>
		<!-- mongo db info -->
		<property>
			<name>mongoHost</name>
			<value>172.18.109.14</value>
		</property>
		<property>
			<name>mongoPort</name>
			<value>27017</value>
		</property>
		<property>
			<name>mongoDbName</name>
			<value>flickr</value>
		</property>
		<property>
			<name>UseProxy</name>
			<value>false</value>
		</property>
		<property>
			<name>datumXml</name>
			<value>conf/column-maps-image.xml</value>
		</property>
	</site>
</configuration>