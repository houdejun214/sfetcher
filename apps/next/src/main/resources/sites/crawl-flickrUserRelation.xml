<?xml version="1.0"?>
<configuration>
	<!-- flickr's userRelation -->
	<site name="flickrUserRelation" hosturl="www.flickr.com"
		fetcher="com.sdata.component.fetcher.FlickrUserRelationFetcher" storer="com.sdata.component.data.SdataFlickrUserRelationDbStorer">
		<property>
			<name>QueueThresholdSizeSlow</name>
			<value>800</value>
			<description></description>
		</property>
		<property>
			<name>FetchThreadNum</name>
			<value>15</value>
		</property>
		<property>
			<name>MainCrawlThreadNum</name>
			<value>15</value>
		</property>
		<property>
			<name>source</name>
			<value>flickr</value>
		</property>
		<property>
			<name>MaxUsersNumPerKey</name>
			<value>1000</value>
			<description></description>
		</property>
		<property>
			<name>StartTime</name>
			<value>2005-04-01 00:00:00</value>
		</property>
		<property>
			<name>EndTime</name>
			<value>2012-04-01 00:00:00</value>
			<description></description>
		</property>
		<!-- mongo db info -->
		<property>
			<name>mongoHost</name>
			<value>172.18.109.14</value>
		</property>
		<property>
			<name>mongoPort</name>
			<value>27017</value>
		</property>
		<property>
			<name>mongoDbName</name>
			<value>flickr</value>
		</property>
		<property>
			<name>UseProxy</name>
			<value>false</value>
		</property>
		<property>
			<name>datumXml</name>
			<value>conf/column-maps-image.xml</value>
		</property>
		<property>
			<name>userSeedFile</name>
			<value>conf/user-seeds-flickr.list</value>
		</property>
	</site>
</configuration>