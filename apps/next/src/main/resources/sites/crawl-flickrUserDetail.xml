<?xml version="1.0"?>
<configuration>
	<!-- flickr's userDetail -->
	<site name="flickrUserDetail" 
		fetcher="com.sdata.component.fetcher.FlickrUserDetailFetcher" storer="com.sdata.core.data.SdataStandardStorer">
		<property>
			<name>QueueThresholdSizeSlow</name>
			<value>800</value>
			<description></description>
		</property>
		<property>
			<name>source</name>
			<value>flickr</value>
		</property>
		<property>
			<name>FetchThreadNum</name>
			<value>1</value>
		</property>
		<property>
			<name>MainCrawlThreadNum</name>
			<value>1</value>
		</property>
		<property>
			<name>MaxUsersNumPerQuery</name>
			<value>100</value>
			<description></description>
		</property>
		<property>
			<name>UseProxy</name>
			<value>false</value>
		</property>
		<property>
			<name>datumXml</name>
			<value>template/flickr/tpl-flickr-image-datum.xml</value>
		</property>
		<property>
			<name>storeXml</name>
			<value>template/flickr/tpl-flickr-image-store.xml</value>
		</property>
	</site>
</configuration>