<?xml version="1.0"?>
<configuration>
	<site name="flickr" hosturl="www.flickr.com" fetcher="com.sdata.component.fetcher.FlickrFetcher"
		storer="com.sdata.component.data.SdataImageDbStorer">
		<property>
			<name>DataDir</name>
			<value>/public/data/images</value>
		</property>
		<property>
			<name>query.input.file</name>
			<value>conf/query.flickr.list</value>
		</property>
		<property>
			<name>FetchThreadNum</name>
			<value>15</value>
		</property>
		<property>
			<name>source</name>
			<value>flickr</value>
		</property>
		<property>
			<name>StartTime</name>
			<value>2012-02-01 00:00:00</value>
		</property>
		<!-- mongo db info -->
		<property>
			  <name>mongoHost</name>
			  <value>172.18.109.14</value>
		</property>
		<property>
		  <name>indexCollection</name>
		  <value>images</value>
		</property>
		<property>
			<name>ImageProcessHost</name>
			<value>172.18.109.14:8080</value>
		</property>
		<property>
			<name>mongoPort</name>
			<value>27017</value>
		</property>
		<property>
			<name>mongoDbName</name>
			<value>flickr</value>
		</property>
		<property>
			<name>datumXml</name>
			<value>conf/column-maps-image.xml</value>
		</property>
		<property>
			<name>pageTemplate</name>
			<value>conf/site-page-flickr.xml</value>
		</property>
		<property>
			<name>TaskInterval</name>
			<value>604800000</value>
		</property>
		<filter pos="before" class="com.sdata.component.filter.SdataDbImageDuplicateFilter"></filter>
	</site>
</configuration>