<?xml version="1.0"?>
<datum filter="null:_id&amp;url&amp;title&amp;content&amp;pub_time">
 	<field name="_id" ref="id"  ></field>
 	<field name="id"  ref="url"  handler="com.sdata.core.parser.html.handler.UuidEncodeHandler"></field>
 	<field name="url" ref="url"></field>
	<field name="title"  selector=".blog_main_left_content h3>a,#artical_topic|txt"></field>
	<field name="content"  selector="#blog_article_content,#artical_real|html" next=".next #pagenext|link"></field>
	<field name="pub_time"  selector=".blog_main_time p,#artical_sth p span:eq(0)|txt" action="todate"></field>
	<field name="fet_time" action="todate"></field>
	<field name="lang" value="zh"></field>

	<field name="image"  selector="#artical_real img|links" action="saveImage"></field>
	<field name="video"></field>
	<field name="uname"></field>
	<field name="uid"></field>
	<field name="geo"></field>
	<field name="loc"></field>
	
	<field name="dtf_s"></field>
	<field name="dtf_pv"></field>
	<field name="dtf_pt"></field>
	<field name="dtf_oid"></field>
	
	<field name="media"  selector="#title01,#artical_sth span:eq(1)|txt"></field>
	<field name="tags"  selector=".blog_nav01 .current,#artical_sth2 a|txt"></field>
</datum>